/* 文件上传的 start */

.upload-file-content {
  display: inline-block;
}

.upload-progress {
  width:100%;
  height:232px;
  background:rgba(255,255,255,1);
  border-radius:5px;
  text-align center
  padding 56px 20px
  > p {
    height:22px;
    font-size:16px;/*no*/
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:22px;
    margin-bottom 68px
  }
}

/* 文件上传的 end */

/* 文件下载的 start */
.cus-dowload {
  display inline-block
}
.cus-dowload-stopclick {
  display inline-block
  pointer-events: none
}
/* 文件下载的 end */
